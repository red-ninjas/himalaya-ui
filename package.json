{
  "name": "@himalaya-ui/core",
  "version": "0.4.10",
  "main": "dist/bundle/index.js",
  "module": "dist/bundle/esm/index.js",
  "types": "dist/bundle/esm/index.d.ts",
  "files": [
    "/dist/bundle",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "yarn docs:collect && next dev ./src",
    "docs:start": "next start src",
    "docs:collect": "node scripts/collect-meta.mjs",
    "docs:build": "yarn docs:collect && cross-env NODE_ENV=production next build ./src && yarn docs:sitemap",
    "docs:sitemap": "node scripts/sitemap.js",
    "prettier": "prettier --write src",
    "lint": "next lint ./src",
    "test": "jest --config .jest.config.js --no-cache --passWithNoTests",
    "test:update": "jest --config .jest.config.js --no-cache --update-snapshot",
    "test:coverage": "yarn test --coverage",
    "build:clear": "rm -rf ./dist/framework ./dist/esm",
    "build:rollup": "rollup -c scripts/rollup.config.js",
    "build:babel": "babel --no-babelrc --config-file ./scripts/babel.config.js --extensions \".js,.ts,.tsx,.jsx,.node,.mjs\" ./src/components --out-dir ./dist/esm --ignore \"**/__tests__/**/*,**/*.d.ts\"",
    "build:after": "node scripts/move-built-in.js",
    "build:types": "ttsc -p ./scripts & ttsc -p ./scripts --outDir ./dist/esm",
    "build:bundle": "node scripts/build-bundle.mjs",
    "build": "yarn build:rollup && yarn build:babel && yarn build:types && yarn build:after && yarn build:bundle",
    "release": "yarn build && yarn build:bundle && cd dist/bundle && yarn build:bundle && yarn publish --access public --non-interactive",
    "prepare": "husky install",
    "icons:update": "ts-node -P scripts/icons scripts/icons/icons-update.ts",
    "icons:generate": "ts-node -P scripts/icons scripts/icons/icons-generate.ts"
  },
  "author": "RedNinjas LTD",
  "license": "MIT",
  "description": "Himalaya is a modern and minimalist UI library for NextJS 13.",
  "homepage": "https://himalaya-ui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/red-ninjas/himalaya-ui.git"
  },
  "bugs": {
    "url": "https://github.com/red-ninjas/himalaya-ui/issues"
  },
  "prettier": "./prettier.json",
  "keywords": [
    "himalaya",
    "himalaya ui",
    "components",
    "react components",
    "react ui"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1-26",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-block-scoping": "^7.23.0",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@fontsource-variable/instrument-sans": "^5.0.3",
    "@jest/globals": "^29.7.0",
    "@mapbox/rehype-prism": "^0.8.0",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/bundle-analyzer": "^13.5.6",
    "@next/eslint-plugin-next": "^13.5.6",
    "@next/mdx": "^13.5.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "~5.0.2",
    "@rollup/plugin-terser": "~0.4.3",
    "@styled-jsx/plugin-sass": "^4.1.0",
    "@svgr/cli": "^8.1.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/user-event": "^14.5.1",
    "@types/glob": "~8.1.0",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.198",
    "@types/mdx": "^2.0.8",
    "@types/mocha": "~10.0.1",
    "@types/node": "~16.18.9",
    "@types/pngjs": "~6.0.1",
    "@types/react": "^18.2.22",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
