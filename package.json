{
  "name": "@himalaya-ui/core",
  "version": "2.4.0",
  "main": "dist/bundle/index.js",
  "module": "dist/bundle/index.js",
  "types": "dist/bundle/index.d.ts",
  "files": [
    "/dist/bundle",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typedoc": "typedoc --tsconfig src/tsconfig.typedoc.json",
    "dev": "npm run docs:collect && next dev ./src",
    "docs:start": "next start src",
    "docs:collect": "node scripts/collect-meta.mjs",
    "docs:build": "npm run attributes:build && npm run docs:collect && cross-env NODE_ENV=production next build ./src && npm run docs:sitemap",
    "docs:sitemap": "node scripts/sitemap.js",
    "prettier": "prettier --write src",
    "lint": "next lint ./src",
    "test": "jest --config .jest.config.js --no-cache --passWithNoTests",
    "test:update": "jest --config .jest.config.js --no-cache --update-snapshot",
    "test:coverage": "npm run test --coverage",
    "build:bundle": "node scripts/build-bundle.mjs",
    "build:esm": "ttsc -p ./src/tsconfig.build.json --outDir ./dist/bundle",
    "build": "npm run build:esm &&  npm run build:bundle",
    "release": "npm run build && cd dist/bundle && npm publish --access public --non-interactive",
    "prepare": "husky install",
    "attributes:generate": "node --loader ts-node/esm scripts/attributes/generate.ts",
    "attributes:build": "npm run typedoc && npm run attributes:generate"
  },
  "author": "RedNinjas LTD",
  "license": "MIT",
  "description": "Himalaya is a modern and minimalist UI library for NextJS 13.",
  "homepage": "https://himalaya-ui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/red-ninjas/himalaya-ui.git"
  },
  "bugs": {
    "url": "https://github.com/red-ninjas/himalaya-ui/issues"
  },
  "prettier": "./prettier.json",
  "keywords": [
    "himalaya",
    "himalaya ui",
    "components",
    "react components",
    "react ui"
  ],
  "sideEffects": false,
  "type": "module",
