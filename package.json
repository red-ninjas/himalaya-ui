  },
  "peerDependencies": {
    "next": ">=13.5.6",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "styled-jsx": ">=5.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "@fontsource-variable/roboto-mono": "^5.0.17",
    "@splidejs/splide": "^4.1.4",
    "axios": "^1.6.2",
    "fancy-canvas": "^2.1.0",
