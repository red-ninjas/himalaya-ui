    "styled-jsx-plugin-sass": "^1.0.0",
    "svgo": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "~10.9.1",
    "ts-transformer-properties-rename": "^0.16.0",
    "ts-transformer-strip-const-enums": "^1.0.1",
    "tslib": "2.5.2",
    "tslint": "^6.1.3",
    "tslint-to-eslint-config": "^2.14.1",
    "ttsc": "^0.3.1",
    "typescript": "^5.2.2",
